{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "yarp": {
    "routes": {
      "auth": {
        "clusterId": "auth",
        "match": {
          "path": "auth/{**catch-all}"
        },
        "transforms": [
          {
            "pathPattern": "{**catch-all}"
          }
        ]
      },
      "ridesharing": {
        "clusterId": "ridesharing",
        "match": {
          "path": "ridesharing/{**catch-all}"
        },
        "transforms": [
          {
            "pathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "clusters": {
      "auth": {
        "destinations": {
          "destination1": {
            "address": "https://localhost:5001"
          }
        }
      },
      "ridesharing": {
        "destinations": {
          "destination1": {
            "address": "https://localhost:5002"
          }
        }
      }
    }
  }
}
